{
    "name": "helth",
    "displayName": "helth",
    "description": "i am become helth",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:helth.helloWorld",
        "onCommand:helth.refresh",
        "onCommand:helth.turnOnTwentyTwentyTwenty",
        "onCommand:helth.turnOffTwentyTwentyTwenty",
        "onCommand:helth.turnOnStandingReminder",
        "onCommand:helth.turnOffStandingReminder",
        "onView:helth-sidebar"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
              {
                "id": "helth-sidebar-view",
                "title": "Helth",
                "icon": "media/helth.svg"
              }
            ]
          },
          "views": {
            "helth-sidebar-view": [
              {
                "type": "webview",
                "id": "helth-sidebar",
                "name": "Helth",
                "icon": "media/helth.svg",
                "contextualTitle": "Helth"
              }
            ]
          },
        "commands": [
            {
                "command": "helth.helloWorld",
                "title": "Hello World",
                "category": "helth"
            },
            {
                "command": "helth.refresh",
                "title": "Refresh",
                "category": "helth"
            },
            {
                "command": "helth.turnOnTwentyTwentyTwenty",
                "title": "Turn on 20/20/20",
                "category": "helth"
            },
            {
                "command": "helth.turnOffTwentyTwentyTwenty",
                "title": "Turn off 20/20/20",
                "category": "helth"
            },
            {
                "command": "helth.turnOnStandingReminder",
                "title": "Turn on Standing Reminder",
                "category": "helth"
            },
            {
                "command": "helth.turnOffStandingReminder",
                "title": "Turn off Standing Reminder",
                "category": "helth"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "concurrently \"rollup -c -w\" \"webpack --watch\"",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-node-resolve": "^13.1.2",
        "@rollup/plugin-typescript": "^8.3.0",
        "@tsconfig/svelte": "^3.0.0",
        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "@types/vscode": "^1.63.0",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "@vscode/test-electron": "^1.6.2",
        "concurrently": "^6.5.1",
        "eslint": "^8.1.0",
        "glob": "^7.1.7",
        "mocha": "^9.1.3",
        "preprocess": "^3.2.0",
        "rollup": "^2.3.4",
        "rollup-plugin-svelte": "^6.0.0",
        "rollup-plugin-terser": "^7.0.2",
        "svelte": "^3.44.3",
        "svelte-check": "^2.2.11",
        "svelte-preprocess": "^4.10.1",
        "ts-loader": "^9.2.5",
        "typescript": "^4.4.4",
        "webpack": "^5.52.1",
        "webpack-cli": "^4.8.0"
    }
}
